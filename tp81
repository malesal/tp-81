#include <bits/stdc++.h>
using namespace std;

bool EncajanDientes(const vector<int> &dientesSuperiores, const vector<int> &dientesInferiores);

int main() {
    int numCasos;
    cin >> numCasos;

    for (int caso = 1; caso <= numCasos; caso++) {
        vector<int> dientesSuperiores(6);
        vector<int> dientesInferiores(6);

        for (int i = 0; i < 6; i++) {
            cin >> dientesSuperiores[i];
        }

        for (int i = 0; i < 6; i++) {
            cin >> dientesInferiores[i];
        }

        if (EncajanDientes(dientesSuperiores, dientesInferiores)) {
            cout << "SI" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
bool EncajanDientes(const vector<int> &dientesSuperiores, const vector<int> &dientesInferiores) {
    for (int i = 0; i < 6; i++) {
        if (dientesSuperiores[i] != dientesInferiores[i]) {
            return false;
        }
    }
    return true;
}
