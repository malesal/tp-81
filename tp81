#include <bits/stdc++.h>
using namespace std;

bool encajanDientes(const vector<int> & superiores, const vector<int> & inferiores);

int main() {
    int numCasos;
    cin >> numCasos;

    for (int caso = 0; caso < numCasos; ++caso) {
        vector<int> superiores(6);
        vector<int> inferiores(6);

        for (int i = 0; i < 6; ++i) {
            cin >> superiores[i];
        }

        for (int i = 0; i < 6; ++i) {
            cin >> inferiores[i];
        }

        if (encajanDientes(superiores, inferiores)) {
            cout << "SI" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
bool encajanDientes(const vector<int> & superiores, const vector<int> & inferiores) {
    for (size_t i = 0; i < superiores.size(); ++i) {
        if (superiores[i] != inferiores[i]) {
            return false;
        }
    }
    return true;
}
